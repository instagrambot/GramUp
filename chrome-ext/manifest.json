{
  "name": "Up! â€“ Free Social Bot",
  "short_name": "Up! Bot",
  "version": "1.7.2",
  "description": "Extension helps you effectively use social networks!",
  "icons": {
    "128": "images/icon128.png",
    "192": "images/icon.png"
  },
  "background": {
    "scripts": [
      "build/background/index.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "storage",
    "background",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "https://i.instagram.com/",
    "https://web.instagram.com/"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "https://dashboard.gramup.me/*",
      "https://dm.gramup.me/*",
      "https://*.okhlopkov.now.sh/*",
      "https://*.likeup.now.sh/*",
      "https://*.caffeinum.now.sh/*",
      "https://*.upme.plus/*"
    ]
  },
  "manifest_version": 2
}
